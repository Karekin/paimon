  <modelVersion>4.0.0</modelVersion>
  <artifactId>paimon-flink-common</artifactId>
  <name>Paimon : Flink : Common</name>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-shade-plugin</artifactId>
        <executions>
          <execution>
            <id>shade-paimon</id>
            <phase>package</phase>
            <goals>
              <goal>shade</goal>
            </goals>
            <configuration>
              <artifactSet>
                <includes>
                  <include>org.apache.paimon:paimon-bundle</include>
                  <include>org.apache.paimon:paimon-service-client</include>
                  <include>org.apache.paimon:paimon-service-runtime</include>
                  <include>org.apache.paimon:paimon-shade-netty-4</include>
                </includes>
              </artifactSet>
            </configuration>
          </execution>
        </executions>
      </plugin>
      <plugin>
        <artifactId>maven-jar-plugin</artifactId>
        <executions>
          <execution>
            <goals>
              <goal>test-jar</goal>
            </goals>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>
